{
  "window.menuBarVisibility": "compact",
  "window.commandCenter": true,
  "window.menuStyle": "custom",
  "window.dialogStyle": "custom",
  "window.restoreWindows": "none",
  "window.title": "${rootName}${profileName}${separator}${appName}",
  "window.titleBarStyle": "custom",
  "window.controlsStyle": "custom",
  "window.zoomPerWindow": true,
  "window.newWindowDimensions": "inherit",
  "window.autoDetectColorScheme": false,
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.iconTheme": "symbols",
  "workbench.activityBar.location": "top",
  "workbench.sideBar.location": "right",
  "workbench.navigationControl.enabled": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.wrapTabs": true,
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.decorations.badges": true,
  "workbench.editor.decorations.colors": true,
  "workbench.hover.delay": 750,
  "workbench.tree.enableStickyScroll": true,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tree.indent": 8,
  "workbench.experimental.share.enabled": true,
  "workbench.colorCustomizations": {
    "[Tokyo Night Storm]": {
      "editorCursor.background": "#000000",
      "editorOverviewRuler.border": "#0000",
      "editorOverviewRuler.background": "#0000",
      "editorOverviewRuler.wordHighlightForeground": "#0000",
      "editorOverviewRuler.wordHighlightStrongForeground": "#0000",
      "editorOverviewRuler.selectionHighlightForeground": "#0000",
      "editorOverviewRuler.rangeHighlightForeground": "#0000",
      "editorOverviewRuler.bracketMatchForeground": "#0000",
      "editorOverviewRuler.findMatchForeground": "#0000",
      "gitDecoration.modifiedResourceForeground": "#e2c08d",
      "gitDecoration.addedResourceForeground": "#9ece6a",
      "gitDecoration.untrackedResourceForeground": "#73daca",
      "gitDecoration.deletedResourceForeground": "#f7768e",
      "editorGutter.modifiedBackground": "#e2c08d",
      "editorGutter.addedBackground": "#9ece6a",
      "editorGutter.deletedBackground": "#f7768e"
    }
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "explorer.compactFolders": false,
  "explorer.decorations.badges": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/.vercel": true
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.fontFamily": "'JetBrains Mono Nerd Font', monospace",
  "editor.fontWeight": "bold",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.fontVariations": false,
  "editor.lineHeight": 1.4,
  "editor.suggestFontSize": 14,
  "editor.suggestLineHeight": 20,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.mouseWheelZoom": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "none",
  "editor.wordWrap": "on",
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": "active",
  "editor.guides.indentation": false,
  "editor.matchBrackets": "never",
  "editor.folding": true,
  "editor.showFoldingControls": "never",
  "editor.cursorBlinking": "expand",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.scrollbar.vertical": "auto",
  "editor.scrollbar.horizontal": "hidden",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.stickyScroll.enabled": true,
  "editor.colorDecorators": false,
  "editor.codeLens": false,
  "editor.links": true,
  "editor.parameterHints.enabled": true,
  "editor.lightbulb.enabled": "off",
  "editor.hover.enabled": "on",
  "editor.hover.delay": 500,
  "editor.occurrencesHighlight": "off",
  "editor.selectionHighlight": false,
  "editor.glyphMargin": false,
  "editor.tabCompletion": "on",
  "editor.snippetSuggestions": "top",
  "editor.lineNumbers": "on",
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "inline",
    "other": "on"
  },
  "editor.copyWithSyntaxHighlighting": false,
  "editor.emptySelectionClipboard": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.gotoLocation.multipleDeclarations": "goto",
  "editor.gotoLocation.multipleImplementations": "goto",
  "editor.gotoLocation.multipleTypeDefinitions": "goto",
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "off",
  "breadcrumbs.symbolPath": "on",
  "breadcrumbs.icons": true,
  "breadcrumbs.symbolSortOrder": "position",
  "search.followSymlinks": false,
  "search.useIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": true,
  "search.smartCase": true,
  "terminal.external.linuxExec": "truecolor",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "'Hack Nerd Font Mono', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontWeightBold": "bold",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.enablePersistentSessions": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.allowChords": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.tabs.hideCondition": "never",
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.tabs.showActiveTerminal": "singleTerminal",
  "terminal.integrated.tabs.showActions": "singleTerminal",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "git.decorations.enabled": true,
  "git.confirmSync": false,
  "git.fetchOnPull": true,
  "git.postCommitCommand": "sync",
  "scm.diffDecorations": "all",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "diffEditor.maxFileSize": 900,
  "diffEditor.wordWrap": "on",
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": true,
    "json": true,
    "yaml": true,
    "toml": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "chat.agent.maxRequests": 125,
  "chat.commandCenter.enabled": false,
  "chat.extensionTools.enabled": false,
  "chat.extensionUnification.enabled": true,
  "problems.decorations.enabled": true,
  "telemetry.telemetryLevel": "off",
  "update.mode": "default",
  "extensions.ignoreRecommendations": true,
  "extensions.autoCheckUpdates": true,
  "extensions.closeExtensionDetailsOnViewChange": true,
  "symbols.hidesExplorerArrows": true,
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.class": "default"
  },
  "markdown-preview-enhanced.previewTheme": "github-dark.css",
  "markdown-preview-enhanced.enablePreviewZenMode": false
}
